<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/appNavGraph"
    app:startDestination="@id/albumNavGraph"
    tools:ignore="UnusedNavigation">

    <include-dynamic
        android:id="@+id/albumNavGraph"
        app:graphResName="albums_nav_graph"
        app:moduleName="feature_albums" />

    <include-dynamic
        android:id="@+id/favoriteNavGraph"
        app:graphResName="favorite_nav_graph"
        app:moduleName="feature_favorite" />

    <fragment
        android:id="@+id/profile_dest"
        android:name="com.wassim.showcase.view.ProfileFragment"
        android:label="ProfileFragment" />

</navigation>